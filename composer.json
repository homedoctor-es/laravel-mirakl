{
    "name": "homedoctor-es/laravel-mirakl",
    "description": "Laravel integration for the Mirakl PHP SDK including shop client wrapper",
    "type": "library",
    "keywords": [
        "laravel",
        "mirakl",
        "marketplace",
        "shop",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Homedoctor",
            "email": "developers@homedoctor.es"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "mirakl/sdk-php-shop": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "HomedoctorEs\\Laravel\\Mirakl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HomedoctorEs\\Laravel\\Mirakl\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HomedoctorEs\\Laravel\\Mirakl\\MiraklServiceProvider"
            ],
            "aliases": {
                "Mirakl": "HomedoctorEs\\Laravel\\Mirakl\\Facades\\Mirakl"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
